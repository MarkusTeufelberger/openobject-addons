<?xml version="1.0" encoding="utf-8"?>
<openerp>
<data>
	<record id="product_supplierinfo_extended_form_view" model="ir.ui.view">
		<field name="name">product.supplierinfo.form.extended1</field>
		<field name="model">product.supplierinfo</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="product.product_supplierinfo_form_view" />	  
		<field name="arch" type="xml">
			<xpath expr="//form/field[@name='price']" position="after">
				<field name="date_start"/>
				<field name="date_end"/>
				<field name="state"/>
				<field name="valid"/>
			</xpath>
		</field>
	</record>
	<record id="product_supplierinfo_extended_tree_view" model="ir.ui.view">
		<field name="name">product.supplierinfo.form.extended2</field>
		<field name="model">product.supplierinfo</field>
		<field name="type">form</field>
		<field name="inherit_id" ref="product.product_supplierinfo_form_view" />	  
		<field name="arch" type="xml">
			<xpath expr="//tree/field[@name='price']" position="after">
				<field name="date_start"/>
				<field name="date_end"/>
				<field name="state"/>
				<field name="valid"/>
			</xpath>
		</field>
	</record>
</data>
</openerp>
