<?xml version="1.0" ?>
<openerp>
    <data>
        <record model="ir.ui.view" id="view_purchase_order_trademark_form">
            <field name="name">purchase.order.trademark.form</field>
            <field name="model">purchase.order</field>
            <field name="inherit_id" ref="purchase.purchase_order_form" />
            <field name="type">form</field>
            <field name="arch" type="xml">
                <xpath expr="/form/notebook/page[@string='Purchase Order']/field[@name='order_line']/tree/field[@name='name']" position="after">
                    <field name="manufacturer"/>
                </xpath>
            </field>
        </record>
    </data>
</openerp>

