<?xml version="1.0" encoding="UTF-8"?>

<openerp>
    <data>
        <record id="account_account_chart_report_view" model="ir.ui.view">
            <field name="name">Chart of accounts</field>
            <field name="model">account.account.chart.report</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
            <form string="Select parent account">
                <field name="account" colspan="4"/>
                <group col="4" colspan="4">
                    <button icon="gtk-cancel" special="cancel" string="Cancel" colspan="2"/>
                    <button name="print_report" string="Print" type="object" icon="gtk-print" default_focus="1"/>
                </group>
            </form>
            </field>
         </record>

    </data>
</openerp>
