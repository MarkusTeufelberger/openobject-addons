    <section>
      <para style="terp_default_8">[[ repeatIn(objects,'o') ]]</para>
      <blockTable colWidths="402.0,136.0" style="Tableau2">
        <tr>
          <td>
            <para style="P9">[[ o.name ]] <font face="Times-Roman" size="8.0">(id: [[ o.id ]])</font></para>
          </td>
          <td>
            <para style="P1">Forme : <font face="Times-Roman">[[ o.title ]]</font></para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P1">N° Entreprise : <font face="Times-Roman">[[ o.vat or 'inconnu' ]]</font><font face="Times-Roman"> [[ o.vat_subjected and 'Assujetti à la TVA' or 'Non-assujetti' ]]</font></para>
          </td>
          <td>
            <para style="P1">Pers. occupées :<font face="Times-Roman"> [[ o.employee_nbr or '0' ]]</font> (global [[ o.employee_nbr_total or '0' ]])</para>
          </td>
        </tr>
        <tr>
          <td>
            <para style="P1">Activité : <font face="Times-Roman">[[ o.activity_description ]]</font></para>
          </td>
        </tr>
      </blockTable>
