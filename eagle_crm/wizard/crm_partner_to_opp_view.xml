<?xml version="1.0" encoding="utf-8"?><!--  File: wizard/crm_partner_to_opp_view.xml  Module: eagle_crm  Created by sbe@open-net.ch  Copyright (c) 2011 Open-Net Ltd. All rights reserved.--><openerp><data>    <record model="ir.ui.view" id="eagle_view_partner2opp_form">        <field name="name">eagle.partner2opp.form</field>        <field name="model">eagle.partner2opp</field>        <field name="type">form</field>        <field name="arch" type="xml">            <form string="New Opportunity">				<group col="4" colspan="4" height="330">	            	<group colspan="4" attrs="{'invisible': [('state','!=','step1')]}" height="300" width="500">	            		<separator string="Opportunity description" colspan="4"/>		                    <label string="Subject"/>		                    <newline/>		                    <field colspan="4" name="name" required="1" nolabel="1"/>		                    <newline/>		                    <label string="Description"/>		                    <newline/>							<field name="description" nolabel="1" colspan="4"/>					</group>					<newline/>	            	<group colspan="4" attrs="{'invisible': [('state','!=','step2')]}" height="300" width="500">	            		<separator string="Partner" colspan="4"/>							<field name="partner_id" string="Partner" colspan="2" />													<field name="partner_contact_id" string="Contact"/>						<separator string="Other stakeholders" colspan="4"/>	                        <field name="partner_ids" colspan="4" nolabel="1"/>                   	</group> 	            	<group colspan="4" attrs="{'invisible': [('state','!=','step3')]}" height="300" width="500">	            		<separator string="Info" colspan="4"/> 							<group colspan="4" col="2">							<field name="probability" />	                    	<field name="user_id" /> 		                    <newline/>    							<field name="planned_revenue" />   	                    	<field name="priority" />			                    <newline/>	                    	<field name="section_id" widget="selection"  />							<field name="contract_id" />							</group>					</group>	            	<group colspan="4" attrs="{'invisible': [('state','!=','step4')]}" height="300" width="500">	            		<separator string="Meeting" colspan="4"/>		                    <field name="meeting_name" select="1" string="Summary" attrs="{'required': [('state','=','step4')]}" colspan="4"/>	                    	<field name="categ_id" widget="selection" string="Meeting Type" colspan="2"/>		                    <newline/>	                    	<field name="date_meeting" string="Start Date" required="1"								on_change="onchange_dates(date_meeting,duration,False,allday)" />							<field name="duration" widget="float_time"								on_change="onchange_dates(date_meeting,duration,False,allday)" />							<field name="date_deadline" string="End Date" required="1"								on_change="onchange_dates(date_meeting,False,date_deadline)" />							<newline/>							<field name="location" />							<field name="alarm_id" string="Reminder" widget="selection" />					</group>	            	<group colspan="4" attrs="{'invisible': [('state','!=','step5')]}" height="300" width="500">	            		<separator string="Call" colspan="4"/>		                    <field name="call_name" select="1" string="Summary" attrs="{'required': [('state','=','step5')]}" colspan="4"/>		                    <field name="date_call" string="Start Date"/>							<field name="categ_call_id" widget="selection" domain="[('object_id.model', '=', 'crm.phonecall')]"								string="Type" attrs="{'required': [('state','=','step5')]}" />		                    <newline/>							<field name="partner_phone"/>	                    	<field name="partner_mobile"/>					</group>                </group>				<newline/>            		<separator string=" " colspan="4"/>					<field name="state" readonly="1" colspan="4" invisible="1"/>					<group col="12" colspan="4" width="500">					<button special="cancel" string="Cancel" icon="gtk-cancel"/>					<button name="go_step1" states="step2" string="Description" type="object" icon="gtk-go-back"/>					<button name="go_step2" states="step1" string="Partners" type="object" icon="gtk-go-forward"/>					<button name="go_step2" states="step3" string="Partner" type="object" icon="gtk-go-back"/>					<button name="go_step3" states="step2" string="Information" type="object" icon="gtk-go-forward"/>					<button name="go_step4" states="step3" string="Meeting" type="object" icon="gtk-go-forward"/>					<button name="go_step5" states="step3" string="Call" type="object" icon="gtk-go-forward"/>  					<button name="go_step3" states="step4,step5" string="Information" type="object" icon="gtk-go-back"/>					<button name="make_opportunity" states="step4" string="Create" type="object" icon="gtk-ok"/>					<button name="make_opportunity" states="step5" string="Create" type="object" icon="gtk-ok"/>				</group>                             </form>        </field>    </record>	<record model="ir.actions.act_window" id="eagle_action_partner2opp">		<field name="name">Create a new opportunity</field>		<field name="type">ir.actions.act_window</field>		<field name="res_model">eagle.partner2opp</field>		<field name="view_type">form</field>		<field name="view_mode">tree,form</field>		<field name="view_id" ref="eagle_view_partner2opp_form"/>		<field name="target">new</field>	</record>			<act_window id="eagle_crm_partner2opportunity"    		key2="client_action_multi" name="New Opportunity"    		res_model="eagle.partner2opp" src_model="res.partner"    		view_id="eagle_view_partner2opp_form"    		view_mode="form" target="new" view_type="form"    		groups="base.group_extended"/></data></openerp>