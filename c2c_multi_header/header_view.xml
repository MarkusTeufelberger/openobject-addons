        <menuitem action="action_header_img" id="menu_header_img" parent="base.menu_action_res_company_tree" sequence="14"/>
        
        <record id="action_header_rml" model="ir.actions.act_window">
            <field name="name">Header RML</field>
            <field name="res_model">c2c.header_rml</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem action="action_header_rml" id="menu_header_rml" parent="base.menu_action_res_company_tree" sequence="14"/>
        
        
    </data>
