<openerp>
    <data>
        <record model="ir.ui.view" id="header_img">
            <field name="name">res.company.header.img</field>
            <field name="model">c2c.header_img</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                    <field name="img" widget="image"/>
                </form>
            </field>
        </record>
        
        <record model="ir.ui.view" id="header_rml">
            <field name="name">res.company.header.rml</field>
            <field name="model">c2c.header_rml</field>
            <field name="type">form</field>
