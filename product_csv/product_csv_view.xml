<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="product_csv_form_view" model="ir.ui.view">
            <field name="name">product.csv.form</field>
            <field name="model">csv.file.field</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="base_csv.csv_file_field_form_view"/>
            <field eval="7" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="/form/field[@name='field_id']" position="after">
                    <field name="sku"/>
                </xpath>
            </field>
        </record>

    </data>
</openerp>
