  <story>
    <para style="Standard">
      <font color="white"> </font>
    </para>
    <para style="terp_default_9">[[ repeatIn(objects, 'o') ]]</para>
    <blockTable colWidths="539.0" style="Table_Heading">
      <tr>
        <td>
          <para style="terp_header_Centre">[[ o.company_id.name ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="539.0" style="Table_Company_Name">
      <tr>
        <td>
          <para style="terp_header_Centre">[[ o.name ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="539.0" style="Table_Date_from_To">
      <tr>
        <td>
          <para style="terp_header_Centre">From : [[ o.st_date ]] To: [[ o.end_date ]]</para>
        </td>
      </tr>
      <tr>
        <td>
          <para style="P2">[[ get_data(o) ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="205.0,118.0,113.0,103.0" style="Table_Account_Line_Title" repeatRows="1">
      <tr>
        <td>
          <para style="terp_tblheader_Details_Centre">[[ o.final_type=='bs' and 'Liability' or 'Expenses' ]]</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Right">Amount([[ o.company_id.currency_id.name ]])</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Right">Amount([[ o.company_id.currency_id.name ]])</para>
        </td>
        <td>
          <para style="terp_tblheader_Details_Right">Total Amount([[ o.company_id.currency_id.name ]])</para>
        </td>
      </tr>
      <tr>
        <td>
          <para style="terp_default_9">
            <font face="Times-Roman">[[ o.final_type=='pl' and repeatIn(get_lines('expense'),'a' ) or removeParentNode('font') ]] </font><font face="Times-Roman">[[ o.final_type=='bs' and repeatIn(get_lines('liability'), 'a') or removeParentNode('font') ]] </font><font color="white">[[  '. '*(a['level']) ]]</font>[[ a['level']==0 and ( setTag('para','para',{'style':'terp_default_Bold_9'})) ]][[ a['name'] ]]
          </para>
        </td>
        <td>
          <para style="terp_default_Right_9">[[ a['outer'] == '-1' and abs(a['balance']) ]]</para>
        </td>
        <td>
          <para style="terp_default_Right_9">[[ a['outer'] == '0' and abs(a['balance']) ]]</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold">[[ a['outer'] == '1' and abs(a['balance']) ]]</para>
        </td>
      </tr>
    </blockTable>
    <blockTable colWidths="205.0,118.0,113.0,103.0" style="Table_Net_Profit_Loss">
      <tr>
        <td>
          <para style="terp_default_Bold_9">Total:([[ o.company_id.currency_id.name ]])</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold">[[ o.final_type=='pl' and '%.2f' % abs(sum_expense_dr()) or '%.2f' % abs(sum_lib1_dr()) ]]</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold">[[ o.final_type=='pl' and '%.2f' % abs(sum_expense_dr()) or '%.2f' % abs(sum_lib_dr()) ]]</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold"><u>[[ o.final_type=='pl' and '%.2f' % abs(sum_dr()) or '%.2f' % abs(sum1_dr()) ]]</u></para>
        </td>
      </tr>
    </blockTable>
    <condPageBreak height="20cm"/>
    <para style="terp_default_Right_9_Bold">
      <font color="white"> </font>
    </para>
