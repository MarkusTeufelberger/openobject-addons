        <td>
          <para style="terp_default_Bold_9">Total:([[ o.company_id.currency_id.name ]])</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold">[[ o.final_type=='pl' and '%.2f' % abs(sum_income1_cr()) or '%.2f' % abs(sum_ass1_cr()) ]]</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold">[[ o.final_type=='pl' and '%.2f' % abs(sum_income_cr()) or '%.2f' % abs(sum_ass_cr()) ]]</para>
        </td>
        <td>
          <para style="terp_default_Right_9_Bold"><u>[[ o.final_type=='pl' and '%.2f' % abs(sum_cr()) or '%.2f' % abs(sum1_cr()) ]]</u></para>
