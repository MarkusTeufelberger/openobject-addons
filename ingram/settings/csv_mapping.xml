<!--Product Template-->
        <record model='csv.file' id='csv02'>
            <field name='name'>product_template</field>
            <field name='path'>[...]/openerp-server6/bin/addons/ingram/temp</field>
            <field name='state'>done</field>
            <field name='file'>PRICE09.TXT</field>
            <field name='file_csv_separator'>,</field>
            <field name='header'>False</field>
            <field name='quote'>"</field>
            <field name='model_id' search="[('model','=','product.template')]" model='ir.model'/>
        </record>

        <record model='csv.file.field' id='csvfpt02'>
            <field name='file_id' ref='ingram.csv02'/>
            <field name='name'>name</field>
            <field name='sequence'>6</field>
            <field name='field_id' search="[('model_id','=','product.template'),('name','=','name')]" model='ir.model.fields'/>
            <field name='ingram_id'>False</field>
            <field name='type'>str</field>
            <field name='import_expression'></field>
        </record>

        <record model='csv.file.field' id='csvfpt03'>
            <field name='file_id' ref='ingram.csv02'/>
            <field name='name'>categ_id</field>
            <field name='sequence'>5</field>
            <field name='field_id' search="[('model_id','=','product.template'),('name','=','categ_id')]" model='ir.model.fields'/>
            <field name='ingram_id'>False</field>
            <field name='type'>int</field>
            <field name='import_expression'>if incsv:
    cat_ids = self.pool.get('product.category').search(cr, uid, [('ingram_code','=',incsv)])
    if len(cat_ids)>0:
        value = cat_ids[0]
    else:
        value = 2
else:
    value = 2</field>
        </record>

        <record model='csv.file.field' id='csvfpt04'>
            <field name='file_id' ref='ingram.csv02'/>
            <field name='name'>description_sale</field>
            <field name='sequence'>7</field>
            <field name='field_id' search="[('model_id','=','product.template'),('name','=','description_sale')]" model='ir.model.fields'/>
            <field name='ingram_id'>False</field>
            <field name='type'>str</field>
            <field name='import_expression'>if incsv:
    value = self.cyr2lat(incsv)</field>
        </record>

        <record model='csv.file.field' id='csvfpt05'>
            <field name='file_id' ref='ingram.csv02'/>
