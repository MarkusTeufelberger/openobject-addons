            <field name='sequence'>24</field>
            <field name='field_id' search="[('model_id','=','product.template'),('name','=','standard_price')]" model='ir.model.fields'/>
            <field name='ingram_id'>False</field>
            <field name='type'>int</field>
            <field name='import_expression'></field>
        </record>

<!--Product Product-->
        <record model='csv.file' id='csv03'>
            <field name='name'>product_product</field>
            <field name='path'>[...]/openerp-server6/bin/addons/ingram/temp</field>
            <field name='state'>done</field>
            <field name='file'>PRICE09.TXT</field>
            <field name='file_csv_separator'>,</field>
            <field name='header'>False</field>
            <field name='quote'>"</field>
            <field name='model_id' search="[('model','=','product.product')]" model='ir.model'/>
        </record>

        <record model='csv.file.field' id='csvfp01'>
            <field name='file_id' ref='ingram.csv03'/>
            <field name='name'>sku</field>
            <field name='sequence'>1</field>
            <field name='field_id' search="[('model_id','=','product.product'),('name','=','ingram_sku')]" model='ir.model.fields'/>
            <field name='ingram_id'>True</field>
            <field name='type'>str</field>
            <field name='import_expression'>value = re.sub('  ','',incsv)[:-1]</field>
        </record>

        <record model='csv.file.field' id='csvfp02'>
            <field name='file_id' ref='ingram.csv03'/>
            <field name='name'>manufacturer</field>
            <field name='sequence'>2</field>
            <field name='field_id' search="[('model_id','=','product.template'),('name','=','manufacturer')]" model='ir.model.fields'/>
            <field name='ingram_id'>False</field>
            <field name='type'>int</field>
            <field name='import_expression'>name = re.sub('  ','',incsv)[:-1]
partner_ids = self.pool.get('res.partner').search(cr, uid, [('name','ilike',name)])
if len(partner_ids)>0:
    value = partner_ids[0]</field>
        </record>

        <record model='csv.file.field' id='csvfp03'>
