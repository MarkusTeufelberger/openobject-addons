<openerp>
<data>

    <record model="ir.ui.view" id="view_lp_project">
        <field name="name">lp.project</field>
        <field name="model">lp.project</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
        	<form>
       		<field name="name"/>
       		<field name="title"/>
       		<field name="summary"/>
       		</form>
       </field>
    </record>


 	<record model="ir.actions.act_window" id="lp_project_form">
        <field name="name">Launchpad Projects</field>
        <field name="res_model">lp.project</field>
        <field name="view_type">form</field>
        <field name="view_id" ref="view_lp_project"/>
 	</record>
    
    <menuitem name="Launchpad Projects" id="menu_lp_projects"  parent="crm_configuration.menu_aftersale" action="lp_project_form"/>
    
    <record model="ir.ui.view" id="view_crm_lp_form">
        <field name="name">crm.case</field>
        <field name="model">crm.case</field>
        <field name="inherit_id" ref="crm_configuration.crm_case_form_view"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
        	<field name="id" position="after">
        		<field name="project_id"/>
        		<field name="lp_project"/>
        		<field name="bug_id"/>
        	</field>
       </field>
    </record>
    
</data>

</openerp>
