    <para style="P1">
      <font face="Helvetica" size="10.0"/>
      <font face="Helvetica" size="10.0">Liège, le </font>
      <font face="Helvetica" size="11.0"/>
      <font face="Times-Roman" size="11.0">[[ time.strftime('%Y-%m-%d') ]]</font>
