	   		<field name="arch" type="xml">
	   			<field name="address_id" position="replace">
		            		<field name="address_id" on_change="onchange_partner_in(address_id,company_id)" context="{'contact_display':'partner'}" colspan="4"/>
			    </field>
	        </field>	
		</record>
	
	</data>
	
</openerp>
