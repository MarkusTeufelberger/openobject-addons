#. module: label
#: model:label.templates,def_body_text:label.label_templates_partner0
msgid ""
"%for o in object.address:\n"
" <blockTable style=\"mytable\">\n"
" <tr><td>\n"
" <b>${object.name or ''}${object.title and ' '+object.title or ''}</b>\n"
" ${o.title and o.title+' ' or ''}${o.name or ''}\n"
" ${o.street or ''}\n"
" ${o.street2 or ''}\n"
" ${o.zip or ''} ${o.city or ''}\n"
" ${o.state_id.name or ''}\n"
" ${o.country_id.name or ''}\n"
" </td></tr>\n"
" </blockTable>\n"
" <nextFrame/>\n"
" %endfor"
msgstr ""
"%for o in object.address:\n"
" <blockTable style=\"mytable\">\n"
" <tr><td>\n"
" <b>${object.name or ''}${object.title and ' '+(object.title=='Corp.' and "
"'S.A.' or object.title=='ltd' and 'S.L.' or object.title) or ''}</b>\n"
" ${o.title and (o.title=='M.' and 'Sr.' or o.title in ['Ms.','Mss'] and "
"'Sra.' or o.title)+' ' or ''}${o.name or ''}\n"
" ${o.street or ''}\n"
" ${o.street2 or ''}\n"
" ${o.zip or ''} ${o.city or ''}\n"
" ${o.state_id.name or ''}\n"
" ${o.country_id.name or ''}\n"
" </td></tr>\n"
" </blockTable>\n"
" <nextFrame/>\n"
" %endfor"

