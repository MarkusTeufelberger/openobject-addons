			<field eval="0" name="dm_action"/>
			<field eval="&quot;&quot;&quot;True&quot;&quot;&quot;" name="condition"/>
			<field eval="&quot;&quot;&quot;Generate Document Job&quot;&quot;&quot;" name="name"/>
		</record>
       
    </data>
</openerp>

