       <!-- Server Action -->
		<record id="ir_actions_server_generatedocumentjob0" model="ir.actions.server">
            <field eval="&quot;&quot;&quot;from dm_remote_edition import dm_remote_edition \naction = dm_remote_edition.generate_document_job(cr, uid, obj.id)&quot;&quot;&quot;" name="code"/>
