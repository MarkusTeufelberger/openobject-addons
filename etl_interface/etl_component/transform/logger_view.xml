<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="etl_component_logger_form" model="ir.ui.view">
			<field name="name">etl.component.logger.form</field>
			<field name="model">etl.component</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="etl_component_form"/>
			<field name="arch" type="xml">
					<notebook position='inside'>
						<page string="Other Information" attrs="{'invisible':[('type_id','!=','transform.logger')]}">
							<field name="output_id" />
						</page>
					</notebook>

			</field>
		</record>

		<record id="action_etl_component_logger_tree"
			model="ir.actions.act_window">
			<field name="name">Logger Component</field>
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('type_id.name','=','transform.logger')]</field>
			<field name="context">{'type_id':'transform.logger'}</field>
		</record>

		<record id="action_etl_component_logger_from"
			model="ir.actions.act_window">
			<field name="res_model">etl.component</field>
			<field name="view_type">form</field>
			<field name="view_mode">form,tree</field>
			<field name="domain">[('type_id.name','=','transform.logger')]</field>
			<field name="context">{'type_id':'transform.logger'}</field>
			<field name="view_id" ref="etl_component_logger_form" />
		</record>

		<record id="action_etl_component_logger_view1"
			model="ir.actions.act_window.view">
			<field eval="1" name="sequence" />
			<field name="view_mode">tree</field>
			<field name="view_id"
				ref="etl_interface.etl_component_tree" />
			<field name="act_window_id"
				ref="action_etl_component_logger_tree" />
		</record>
		<record id="action_etl_component_logger_view2"
			model="ir.actions.act_window.view">
			<field eval="2" name="sequence" />
			<field name="view_mode">form</field>
			<field name="view_id" ref="etl_component_logger_form" />
			<field name="act_window_id"
				ref="action_etl_component_logger_tree" />
		</record>

		<menuitem name="Logger"
			parent="etl_interface.menu_etl_jd_c_pec_form"
			action="action_etl_component_logger_tree"
			id="menu_etl_jd_c_pec__logger_form" />

	</data>
</openerp>