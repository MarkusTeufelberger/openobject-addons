<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!--Sequence types for Delivery Route-->

        <record id="seq_type_delivery_route" model="ir.sequence.type">
            <field name="name">Delivery Route</field>
            <field name="code">delivery.route</field>
        </record>

        <!--Sequences for Delivery Route-->

        <record id="seq_delivery_route" model="ir.sequence">
            <field name="name">Delivery Route</field>
            <field name="code">delivery.route</field>
            <field name="prefix">RT-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
