<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

<!--Page Sizes-->
        <record id="report_pagesize_labelroll" model="report.pagesize">
            <field name="name" eval="&quot;Label roll&quot;"/>
            <field name="width" eval="&quot;13cm&quot;"/>
            <field name="height" eval="&quot;30cm&quot;"/>
        </record>

<!--Label Manufacturers-->
        <record id="report_label_manufacturer_bixolon" model="report.label.manufacturer">
            <field name="name" eval="&quot;Bixolon&quot;"/>
        </record>

<!--Labels-->
