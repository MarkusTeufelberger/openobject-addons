            <field name="height_incr" eval="&quot;2.7cm&quot;"/>
            <field name="rows" eval="1"/>
            <field name="cols" eval="3"/>
            <field name="margin_left" eval="&quot;0.1cm&quot;"/>
            <field name="margin_top" eval="&quot;0cm&quot;"/>
        </record>
    </data>
</openerp>
